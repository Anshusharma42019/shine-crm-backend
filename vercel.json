{
  "crons": [
  {
    "path": "/api/cron-job",
    "schedule": "30 5 * * *"
  },
  {
    "path": "/api/cron-job",
    "schedule": "30 6 * * *"
  },
  {
    "path": "/api/cron-job",
    "schedule": "30 7 * * *"
  },
  {
    "path": "/api/cron-job",
    "schedule": "30 8 * * *"
  }
],
  "routes": [
    { "src": "/api/cron-job", "dest": "api/cron-job.js" },
    { "src": "/(.*)", "dest": "server.js" }
  ],
  "builds": [
    { "src": "server.js", "use": "@vercel/node" },
    { "src": "api/cron-job.js", "use": "@vercel/node" }
  ],
  "version": 2
}
